    "monitors": [
        {
            "eval": "Services.appinfo.name",
            "targetAttr": "appname"
        },
        {
            "eval": "Services.appinfo.version",
            "targetAttr": "appversion"
        },
        {
            "eval": "Services.appinfo.defaultUpdateChannel",
            "targetAttr": "appchannel"
        },
        {
            "pref": "userChromeJS.Sidra.ControlButton.Style",
            "targetAttr": "sidra-controlstyle",
            "defaultValue": "default"
        },
        {
            "pref": "userChromeJS.Sidra.Tabs.Style",
            "targetAttr": "sidra-tabstyle",
            "defaultValue": "default"
        },
        {
            "pref": "floorp.tabbar.style",
            "targetAttr": "tabbarstyle",
            "defaultValue": "0",
            "isAvailable": "Services.appinfo.name === 'Floorp'"
        },
        {
            "pref": "floorp.browser.tabbar.settings",
            "targetAttr": "tabposition",
            "defaultValue": "0",
            "isAvailable": "Services.appinfo.name === 'Floorp'"
        },
        {
            "pref": "userChromeJS.Sidra.Tabs.AllTabsBtn.Visibility",
            "target": "#alltabs-button",
            "targetAttr": "visibility",
            "defaultValue": "auto"
        },
        {
            "eval": "!Services.prefs.getBoolPref('userChromeJS.Sidra.MenuPopup.Compact', false)",
            "target": "#contentAreaContextMenu",
            "targetAttr": "photoncompact"
        },
        {
            "eval": "getComputedStyle(document.getElementById('nav-bar')).height",
            "target": "#navigator-toolbox",
            "timeOut": 100,
            "targetStyle": "--navbar-height"
        },
        {
            "eval": "getComputedStyle(document.getElementById('TabsToolbar')).height",
            "target": "#navigator-toolbox",
            "targetStyle": "--tabs-height"
        },
        {
            "start": "document.getElementById('navigator-toolbox').style.setProperty('--control-width-default', getComputedStyle(document.querySelector('.titlebar-buttonbox-container')).width)",
            "stop": "document.getElementById('navigator-toolbox').style.removeProperty('--control-width-default')"
        },
        {
            "start": "setTimeout(() => {var urlbar = document.getElementById('urlbar'); var toolbox = document.getElementById('navigator-toolbox'); toolbox.style.setProperty('--urlbar-height', urlbar.style.getPropertyValue('--urlbar-height')); toolbox.style.setProperty('--urlbar-toolbar-height', urlbar.style.getPropertyValue('--urlbar-toolbar-height'));}, 50)",
            "stop": "document.getElementById('navigator-toolbox').style.removeProperty('--urlbar-height'); document.getElementById('navigator-toolbox').style.removeProperty('--urlbar-toolbar-height')"
        },
        {
            "from": "toolbarbutton[data-l10n-id=\"show-TST-sidebar\"]",
            "attr": "checked",
            "targetAttr": "tstopened"
        },
        {
            "from": "#sidebar-box",
            "attr": "sidebarcommand",
            "targetAttr": "sidebarCommand"
        },
        {
            "from": "#sidebar-box",
            "attr": "hidden",
            "targetAttr": "sidebarHidden"
        }
    ],
